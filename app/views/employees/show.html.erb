<h1>Name: <%= "#{@employee.first_name} #{@employee.last_name}" %></h1>
<ul>
  <li>Alias: <%= @employee.alias %></li>
  <li>Title: <%= @employee.title %></li>
  <li>Office: <%= @employee.office %></li>
  <li>Dog: <%= link_to(@employee.dog.name, dog_path(@employee.dog)) %></li>
</ul>

<%= link_to('Edit Employee', edit_employee_path(@employee)) %>